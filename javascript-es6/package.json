{
    "name": "@keyclic/sdk-javascript",
    "version": "1.41.3",
    "description": "Javascript client for Keyclic API",
    "author": "Keyclic techies team <techies@keyclic.com>",
    "dependencies": {
        "es6-promise": "^4.2.5",
        "isomorphic-fetch": "^2.2.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.5",
        "@babel/core": "^7.1.6",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/preset-env": "^7.1.6",
        "prettier": "^1.15.2"
    },
    "homepage": "https://keyclic.com",
    "license": "MIT",
    "main": "bundle/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/keyclic/app-sdk.git"
    },
    "scripts": {
        "build": "babel src --out-dir bundle",
        "prettier": "prettier --write bundle/**/*.js src/**/*.js",
        "test": "mocha --compilers js:@babel/core/register --recursive"
    },
    "babel": {
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ],
        "presets": [
            [
                "@babel/env",
                {
                    "targets": "> 0.5%, last 1 ie versions, last 1 Safari versions, not dead"
                }
            ]
        ]
    }
}
