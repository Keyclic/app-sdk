{
    "name": "@keyclic/sdk-javascript",
    "version": "2.0.7",
    "description": "Javascript client for Keyclic API",
    "author": "Keyclic techies team <techies@keyclic.com>",
    "dependencies": {
        "core-js": "^3.2.1",
        "regenerator-runtime": "^0.13.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.6.0",
        "@babel/core": "^7.6.0",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/preset-env": "^7.6.0",
        "prettier": "^1.18.2"
    },
    "homepage": "https://keyclic.com",
    "license": "MIT",
    "main": "bundle/index.js",
    "module": "src/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/keyclic/app-sdk.git"
    },
    "scripts": {
        "build": "babel src --out-dir bundle",
        "prettier": "prettier --write bundle/*.js bundle/**/*.js src/*.js src/**/*.js",
        "test": "mocha --compilers js:@babel/core/register --recursive"
    },
    "babel": {
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ],
        "presets": [
            [
                "@babel/env",
                {
                    "targets": "> 0.5%, last 1 ie versions, last 1 Safari versions, not dead",
                    "useBuiltIns": "entry",
                    "corejs": 3
                }
            ]
        ]
    }
}
